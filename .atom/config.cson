'*':
  'editor':
    invisibles: {}
    showInvisibles: true
    showIndentGuide: true
    fontSize: 12
    lineHeight: 1.45
    scrollPastEnd: true
    fontFamily: "Menlo"
  'core':
    'themes': [
      'solarized-dark-ui'
      'solarized-dark-syntax'
    ]
  'hideGitIgnoredFiles': true
  'ignoredNames': [
      '.git'
      '.svn'
      '.DS_Store'
      '**/nodes_modules/*'
      '**/bower_components/*'
    ]
  'welcome':
    'showOnStartup': false
